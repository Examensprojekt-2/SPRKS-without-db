generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}


model Persons {
  PersonID  Int @id @default(autoincrement())
  LastName  String? @db.VarChar(255)
  FirstName String? @db.VarChar(255)
  Address   String? @db.VarChar(255)
  City      String? @db.VarChar(255)
}
